{
  "$schema": "https://biomejs.dev/schemas/1.8.1/schema.json",
  "extends": ["../../biome.json"],
  "files": {
    "ignore": ["./example/", "./coverage/"]
  },
  "linter": {
    "rules": {
      "performance": {
        "noAccumulatingSpread": "off"
      },
      "complexity": {
        "noBannedTypes": "off"
      },
      "suspicious": {
        "noConfusingVoidType": "off"
      }
    }
  }
}
