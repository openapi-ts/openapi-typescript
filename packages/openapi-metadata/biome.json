{
  "root": false,
  "$schema": "https://biomejs.dev/schemas/2.2.6/schema.json",
  "extends": "//",
  "files": {
    "includes": ["**", "!dist", "!example/", "!coverage/"]
  },
  "linter": {
    "rules": {
      "performance": {
        "noAccumulatingSpread": "off"
      },
      "complexity": {
        "noBannedTypes": "off"
      },
      "suspicious": {
        "noConfusingVoidType": "off"
      }
    }
  }
}
