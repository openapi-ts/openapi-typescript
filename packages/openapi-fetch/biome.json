{
  "$schema": "https://biomejs.dev/schemas/1.8.1/schema.json",
  "extends": ["../../biome.json"],
  "files": {
    "include": ["./src/", "./test/"],
    "ignore": ["**/fixtures/**/*"]
  },
  "linter": {
    "rules": {
      "complexity": {
        "noBannedTypes": "off"
      },
      "suspicious": {
        "noConfusingVoidType": "off"
      }
    }
  }
}
