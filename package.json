{
  "name": "@manifoldco/swagger-to-ts",
  "description": "Generate TypeScript types from Swagger OpenAPI specs",
  "main": "dist/cjs",
  "version": "1.2.2",
  "engines": {
    "node": ">= 8.0.0"
  },
  "bin": {
    "swagger-to-ts": "bin/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manifoldco/swagger-to-ts.git"
  },
  "keywords": [
    "swagger",
    "typescript",
    "ts",
    "openapi",
    "node"
  ],
  "author": "drew@pow.rs",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/manifoldco/swagger-to-ts/issues"
  },
  "homepage": "https://github.com/manifoldco/swagger-to-ts#readme",
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg"
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-copy-assets",
        {
          "files": [
            "bin/"
          ]
        }
      ]
    ]
  },
  "scripts": {
    "build": "pack build",
    "pregenerate": "npm run build",
    "generate": "node pkg/bin/cli example/input.yaml -o example/output.d.ts",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts src",
    "prepublish": "npm run build",
    "publish": "pack publish",
    "pretest": "npm run build",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "fs-extra": "^8.0.1",
    "js-yaml": "^3.13.1",
    "meow": "^5.0.0",
    "prettier": "^1.18.2"
  },
  "devDependencies": {
    "@pika/pack": "^0.5.0",
    "@pika/plugin-build-node": "^0.6.0",
    "@pika/plugin-copy-assets": "^0.5.1",
    "@pika/plugin-ts-standard-pkg": "^0.4.0",
    "@types/jest": "^24.0.13",
    "@types/js-yaml": "^3.12.1",
    "@types/prettier": "^1.16.4",
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "tslib": "^1.10.0",
    "typescript": "^3.5.3"
  }
}
